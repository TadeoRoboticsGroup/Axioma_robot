<?xml version="1.0" ?>
<sdf version="1.6">
  <model name='Axioma'>
    <pose>0 0 0 0 0 0</pose>

    <link name='base_link'>
    <pose>0 0 0 1.5707 0 0</pose>
    <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>0.0227</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz> 
          <iyy>0.0852</iyy>
          <iyz>0</iyz>
          <izz>0.1041</izz>
        </inertia>
      </inertial>
      <visual name='visual'> 
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://axioma/meshes/chasiss.obj</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>model://dolly/materials/scripts</uri>
            <uri>model://dolly/materials/textures</uri>
            <name>Dolly/fur</name>
          </script>
        </material>
      </visual>
      <visual name='visual'> 
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://axioma/meshes/chasiss.obj</uri>
          </mesh>
        </geometry>
      </visual>
    </link>

    <link name="base_scan">
    <pose>0 0 0.15 0 0 1.5708</pose>
        <inertial> 
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0.000</ixy>
            <ixz>0.000</ixz>
            <iyy>0.001</iyy>
            <iyz>0.000</iyz>
            <izz>0.001</izz>
          </inertia>
          <mass>0.125</mass>
        </inertial>
        <collision name="lidar_sensor_collision"> 
          <geometry>
            <cylinder>
              <radius>0.035</radius>
              <length>0.030</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="lidar_sensor_visual"> 
          <geometry>
            <cylinder>
              <radius>0.035</radius>
              <length>0.030</length>
            </cylinder>
          </geometry>
        </visual>
        <sensor name="hls_lfcd_lds" type="ray">
          <pose>0 0 0 0 0 0</pose>
          <always_on>true</always_on>
          <visualize>true</visualize> 
          <update_rate>5</update_rate>
          <ray>
            <scan>
              <horizontal>
                <samples>360</samples>
                <resolution>1.000000</resolution>
                <min_angle>0.000000</min_angle>
                <max_angle>6.280000</max_angle>
              </horizontal>
            </scan>
            <range>
              <min>0.120000</min>
              <max>3.5</max>
              <resolution>0.001000</resolution>
            </range>
            <noise>
              <type>gaussian</type>
              <mean>0.0</mean>
              <stddev>0.01</stddev>
            </noise>
          </ray>
          <plugin name="turtlebot3_laserscan" filename="libgazebo_ros_ray_sensor.so">
            <ros>
              <namespace>/axioma</namespace> 
                <remapping>~/out:=scan</remapping>
            </ros>
            <output_type>sensor_msgs/LaserScan</output_type>
            <frame_name>base_link</frame_name>
          </plugin>  
        </sensor>
        <sensor name="intel_realsense_r200_depth" type="depth">
          <always_on>1</always_on>
          <update_rate>5</update_rate>
          <pose>0 0.4 0 -1.5708 0 0</pose>
          <camera name="realsense_depth_camera">
          </camera>
          <plugin name="intel_realsense_r200_depth_driver" filename="libgazebo_ros_camera.so">
            <ros>
            </ros>
            <camera_name>intel_realsense_r200_depth</camera_name>
            <frame_name>camera_depth_frame</frame_name>
            <hack_baseline>0.07</hack_baseline>
            <min_depth>0.001</min_depth>
            <max_depth>5.0</max_depth>
          </plugin>
        </sensor>
      </link>

    <link name="imu_link">
    <pose>0 0 0.12 0 0 0</pose>
        <sensor name="tb3_imu" type="imu">
          <always_on>true</always_on>
          <update_rate>200</update_rate>
          <imu>
            <angular_velocity>
              <x>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>2e-4</stddev>
                </noise>
              </x>
              <y>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>2e-4</stddev>
                </noise>
              </y>
              <z>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>2e-4</stddev>
                </noise>
              </z>
            </angular_velocity>
            <linear_acceleration>
              <x>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>1.7e-2</stddev>
                </noise>
              </x>
              <y>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>1.7e-2</stddev>
                </noise>
              </y>
              <z>
                <noise type="gaussian">
                  <mean>0.0</mean>
                  <stddev>1.7e-2</stddev>
                </noise>
              </z>
            </linear_acceleration>
          </imu>
          <plugin name="turtlebot3_imu" filename="libgazebo_ros_imu_sensor.so">
            <ros>
              <namespace>/axioma</namespace>
              <remapping>~/out:=imu</remapping>
            </ros>
          </plugin>
        </sensor>
      </link>

      <link name="camera_link"> 
        <pose>-0.14 0 0.12 -3.141516 0 0</pose>
        <inertial> 
          <inertia>
            <ixx>0.001</ixx>
